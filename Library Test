package org.example;

import org.junit.jupiter.api.Test;

import static org.junit.jupiter.api.Assertions.*;

class LibraryTest {
    @Test
    void checkAvailability(){
        assertFalse(Library.isAvailable("Harry Potter"));
        assertTrue(Library.isAvailable("Alice in Wonderland"));
        assertFalse(Library.isAvailable("Fairy Tale"));
    }
}